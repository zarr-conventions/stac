{
  "zarr_format": 3,
  "node_type": "group",
  "attributes": {
    "zarr_conventions": [
      {
        "name": "stac:",
        "spec_url": "https://github.com/zarr-conventions/stac/blob/v1/README.md"
      }
    ],
    "stac:encoding": "json",
    "stac:item": {
      "type": "Feature",
      "stac_version": "1.1.0",
      "id": "example-item",
      "geometry": {
        "type": "Point",
        "coordinates": [0, 0]
      },
      "bbox": [-1, -1, 1, 1],
      "properties": {
        "datetime": "2024-01-01T00:00:00Z"
      },
      "links": [
        {
          "rel": "self",
          "href": "https://example.com/item.json"
        }
      ],
      "assets": {
        "data": {
          "href": "data",
          "type": "application/vnd.zarr; version=3",
          "roles": ["data"]
        }
      }
    }
  }
}
